{
    "name": "mateusjunges/laravel-pix",
    "description": "PIX integration with Laravel",
    "type": "library",
    "keywords": [
        "laravel",
        "junges",
        "pix",
        "bacen",
        "pix-api"
    ],
    "require": {
        "php": "^7.4|^8.0",
        "mpdf/qrcode": "^1.1",
        "illuminate/support": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0",
        "guzzlehttp/guzzle": "^7.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "mateusjunges",
            "email": "mateus@junges.dev"
        }
    ],
    "autoload": {
        "psr-4": {
            "Junges\\Pix\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Junges\\Pix\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Junges\\Pix\\Providers\\PixServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests"
    }
}
